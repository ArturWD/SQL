set serveroutput on;
declare
    eqArray EquationSolver.EquationsArray;
begin
    EquationSolver.SolveEquation(6);
    
    select *
    into eqArray(1)
    from Equations
    where eqIndex =1;
    
    select *
    into eqArray(2)
    from Equations
    where eqIndex =2;
    
    select *
    into eqArray(3)
    from Equations
    where eqIndex =3;
    
    select *
    into eqArray(5)
    from Equations
    where eqIndex =5;
    
    EquationSolver.SolveEquation(eqArray);
end;